{
  "lerna": "3.16.4",
  "command": {
    "version": {
      "allowBranch": [
        "master",
        "release*"
      ],
      "message": "[*] chore(release): %s"
    }
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "packages": [
    "e2e/test-env",
    "examples/main",
    "examples/storybook",
    "packages/**/*",
    "bundle-analyzer",
    "docs"
  ],
  "npmClientArgs": [
    "--no-lockfile"
  ],
  "version": "8.1.2"
}
