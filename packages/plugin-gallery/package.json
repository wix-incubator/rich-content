{
  "name": "wix-rich-content-plugin-gallery",
  "version": "1.0.27",
  "author": {
    "name": "Jonathan Bachman",
    "email": "jonathanb@wix.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix-incubator/rich-content.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wix-incubator/rich-content/issues"
  },
  "homepage": "https://github.com/wix-incubator/rich-content/tree/master/rich-content-plugin-gallery",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "babel:icons": "BABEL_ENV=commonjs babel -q dist/statics -d dist/statics",
    "babel:es": "babel -q src -d dist/es --source-maps",
    "babel:commonjs": "BABEL_ENV=commonjs babel -q src -d dist/lib",
    "prebuild": "npm run clean",
    "build": "npm run babel:es && npm run babel:commonjs",
    "postbuild": "npm run statics",
    "statics": "mkdir -p dist && cp -R statics dist/statics",
    "poststatics": "npm run icons",
    "icons": "svgr -d dist/statics/icons dist/statics/icons",
    "posticons": "create-index -r ./dist/statics/icons && npm run babel:icons",
    "prewatch": "npm run clean && npm run statics",
    "watch": "npm run babel:es -- --watch",
    "release": "wnpm-release --no-shrinkwrap",
    "test": "eslint 'src/**/*.{js,jsx}'"
  },
  "peerDependencies": {
    "@wix/draft-js": "^0.10.105",
    "classnames": "^2.0.0",
    "lodash": "^4.0.0",
    "prop-types": "^15.0.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/cli": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "create-index": "^2.3.0",
    "eslint": "^4.19.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "focus-trap-react": "^3.1.2",
    "image-client-api": "^1.195.0",
    "pro-gallery-renderer": "^1.0.177",
    "react-sortable-hoc": "^0.6.8",
    "wix-rich-content-common": "^1.0.27"
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  }
}
