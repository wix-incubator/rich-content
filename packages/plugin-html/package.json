{
  "name": "wix-rich-content-plugin-html",
  "version": "1.0.27",
  "author": {
    "name": "Jonathan Bachman",
    "email": "jonathanb@wix.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix-incubator/rich-content.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wix-incubator/rich-content/issues"
  },
  "homepage": "https://github.com/wix-incubator/rich-content/tree/master/rich-content-plugin-html",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "babel:es": "babel -q src -d dist/es --source-maps --copy-files",
    "babel:commonjs": "BABEL_ENV=commonjs babel -q src -d dist/lib --copy-files",
    "prebuild": "npm run clean",
    "statics": "mkdir -p dist && cp -R statics dist/statics",
    "build": "npm run babel:es && npm run babel:commonjs",
    "postbuild": "npm run statics",
    "prewatch": "npm run clean && npm run statics",
    "watch": "npm run babel:es -- --watch",
    "release": "wnpm-release --no-shrinkwrap",
    "test": "eslint 'src/**/*.{js,jsx}'"
  },
  "peerDependencies": {
    "@wix/draft-js": "^0.10.105",
    "classnames": "^2.0.0",
    "lodash": "^4.0.0",
    "prop-types": "^15.0.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "eslint": "^4.19.1"
  },
  "dependencies": {
    "react-i18next": "^7.6.1",
    "wix-rich-content-common": "^1.0.27"
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  }
}
