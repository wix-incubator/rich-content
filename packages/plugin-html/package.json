{
  "name": "wix-rich-content-plugin-html",
  "version": "1.0.27",
  "author": {
    "name": "Jonathan Bachman",
    "email": "jonathanb@wix.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix-incubator/rich-content.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wix-incubator/rich-content/issues"
  },
  "homepage": "https://github.com/wix-incubator/rich-content/tree/master/rich-content-plugin-html",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "transpile": "babel -q src -d dist/lib --copy-files",
    "webpack": "webpack --env.FILE_NAME=wix-rich-content-plugin-html",
    "prebuild": "npm run transpile",
    "build:dev": "npm run transpile && npm run webpack -- --config ./config/webpack.dev.js",
    "build": "npm run webpack -- --config ./config/webpack.prod.js",
    "watch": "npm run build:dev -- --watch",
    "watch:prod": "npm run build -- --watch",
    "release": "wnpm-release --no-shrinkwrap",
    "test": "eslint 'src/**/*.{js,jsx}'"
  },
  "peerDependencies": {
    "@wix/draft-js": "^0.10.105",
    "classnames": "^2.0.0",
    "lodash": "^4.0.0",
    "prop-types": "^15.0.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.19.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4"
  },
  "dependencies": {
    "react-i18next": "^7.6.1",
    "wix-rich-content-common": "^1.0.27"
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  }
}
