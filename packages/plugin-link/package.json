{
  "name": "wix-rich-content-plugin-link",
  "version": "2.0.0",
  "author": {
    "name": "Jonathan Bachman",
    "email": "jonathanb@wix.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix-incubator/rich-content.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wix-incubator/rich-content/issues"
  },
  "homepage": "https://github.com/wix-incubator/rich-content/tree/master/rich-content-plugin-link",
  "files": [
    "dist"
  ],
  "main": "dist/module.cjs.js",
  "module": "dist/module.js",
  "style": "dist/styles.min.scss",
  "scripts": {
    "clean": "rm -rf dist/",
    "statics": "mkdir -p dist && cp -R statics dist/statics",
    "prebuild": "npm run statics",
    "build": "rollup --environment MODULE_NAME:plugin-link -c ../../rollup.config.js",
    "build:dev": "NODE_ENV=development npm run build",
    "build:analyze": "npm run build -- --environment MODULE_ANALYZE && open stats.html",
    "watch": "NODE_ENV=development MODULE_WATCH=1 npm run build -- --watch",
    "release": "wnpm-release --no-shrinkwrap",
    "test": "eslint 'src/**/*.{js,jsx}'"
  },
  "peerDependencies": {
    "@babel/runtime": "7.2.0",
    "@wix/draft-js": "0.10.272",
    "classnames": "^2.0.0",
    "lodash": "^4.0.0",
    "prop-types": "^15.0.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.2.0",
    "@wix/draft-js": "0.10.272",
    "classnames": "^2.2.6",
    "eslint": "^4.19.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "rollup": "^1.1.0"
  },
  "dependencies": {
    "draft-js-linkify-plugin": "^2.0.1",
    "wix-rich-content-common": "^2.0.0"
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
