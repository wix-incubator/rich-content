    "link-preview",
    "map",
    "mention",
    "poll",
    "preview",
    "schema",
    "spoiler",
    "table",
    "text-color",
    "text-selection-toolbar",
    "toolbars",
    "undo-redo",
    "unsupported-blocks",
    "vertical-embed",
    "video",
    "viewer"
  ],
  "scripts": {
    "clean": "find . -maxdepth 1 -type d -regex \"\\./.*\" -not -path \"./src*\" -not -path \"./node_modules*\"  -exec rm -rf {} \\;",
    "prebuild": "yarn run clean",
    "buildESM": "tsc",
    "buildCJS": "tsc --module commonjs --outDir dist",
    "build": "yarn buildESM",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,d.ts}'",
    "test": ":"
  },
  "dependencies": {
    "ts-is-present": "^1.1.1",
    "wix-rich-content-plugin-accordion": "8.24.0",
    "wix-rich-content-plugin-button": "8.24.0",
    "wix-rich-content-plugin-code-block": "8.24.0",
    "wix-rich-content-plugin-commons": "8.24.0",
    "wix-rich-content-plugin-divider": "8.24.0",
    "wix-rich-content-plugin-emoji": "8.24.0",
    "wix-rich-content-plugin-file-upload": "8.24.0",
    "wix-rich-content-plugin-gallery": "8.24.0",
    "wix-rich-content-plugin-giphy": "8.24.0",
    "wix-rich-content-plugin-hashtag": "8.24.0",
    "wix-rich-content-plugin-headers-markdown": "8.24.0",
    "wix-rich-content-plugin-headings": "8.24.0",
    "wix-rich-content-plugin-html": "8.24.0",
    "wix-rich-content-plugin-image": "8.24.0",
    "wix-rich-content-plugin-indent": "8.24.0",
    "wix-rich-content-plugin-line-spacing": "8.24.0",
    "wix-rich-content-plugin-link": "8.24.0",
    "wix-rich-content-plugin-link-preview": "8.24.0",
    "wix-rich-content-plugin-map": "8.24.0",
    "wix-rich-content-plugin-mentions": "8.24.0",
    "wix-rich-content-plugin-social-polls": "8.24.0",
    "wix-rich-content-plugin-sound-cloud": "8.24.0",
    "wix-rich-content-plugin-spoiler": "8.24.0",
    "wix-rich-content-plugin-table": "8.24.0",
    "wix-rich-content-plugin-text-color": "8.24.0",
    "wix-rich-content-plugin-undo-redo": "8.24.0",
    "wix-rich-content-plugin-vertical-embed": "8.24.0",
    "wix-rich-content-plugin-video": "8.24.0",
    "wix-rich-content-preview": "8.24.0",
    "wix-rich-content-text-selection-toolbar": "8.24.0"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.jsx?$": "<rootDir>/../../../babel.jest.monorepo.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
