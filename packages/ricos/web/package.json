  "bugs": {
    "url": "https://github.com/wix/ricos/issues"
  },
  "homepage": "https://github.com/wix/ricos/tree/master/ricos",
  "files": [
    "accordion",
    "button",
    "code-block",
    "common",
    "content",
    "divider",
    "driver",
