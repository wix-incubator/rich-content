  "peerDependencies": {
    "immutable": "^3.8.1",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "wix-rich-content-viewer": "^1.0.6"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:analyze": "npm run build -- --progress --env.analyzeBundle",
    "test": "cross-env CI=true node scripts/test.js --env=jsdom",
    "bootstrap": "npm install && npm run link",
    "link": "npm link wix-rich-content-viewer --only=production"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "react-dev-utils": "^5.0.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "1.3.2",
    "eslint": "^4.19.1",
    "eslint-config-wix": "^1.1.18",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-lodash": "^2.7.0",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.8.3",
    "wix-statics-parent": "*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react"
    ]
  }
}
