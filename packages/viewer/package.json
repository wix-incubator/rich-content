{
  "name": "wix-rich-content-viewer",
  "version": "1.1.27",
  "author": {
    "name": "Jonathan Bachman",
    "email": "jonathanb@wix.com"
  },
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist/",
    "babel:es": "babel -q src -d dist/es --source-maps --copy-files",
    "babel:commonjs": "BABEL_ENV=commonjs babel -q src -d dist/lib --copy-files",
    "prebuild:dev": "npm run clean",
    "build:dev": "npm run babel:es",
    "prebuild": "npm run clean",
    "build": "npm run babel:es && npm run babel:commonjs",
    "prewatch": "npm run clean",
    "watch": "npm run babel:es -- --watch",
    "release": "wnpm-release --no-shrinkwrap",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "jest --verbose"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^draft-js$": "<rootDir>/node_modules/@wix/draft-js/dist/Draft.min.js",
      "^draft-js/.*$": "<rootDir>/node_modules/@wix/draft-js/dist/Draft.min.js",
      "^@wix/draft-js$": "<rootDir>/node_modules/@wix/draft-js/dist/Draft.min.js"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "decorate-component-with-props": "^1.1.0",
    "draft-js-drag-n-drop-plugin": "^2.0.1",
    "draft-js-focus-plugin": "^2.0.1",
    "draft-js-plugins-editor": "^2.0.1",
    "focus-trap-react": "^3.1.2",
    "i18next": "^10.5.0",
    "linkify-it": "^2.0.3",
    "lodash": "^4.17.10",
    "photography-client-lib": "^1.0.69",
    "react-i18next": "^7.6.1",
    "redraft": "^0.9.0",
    "wix-rich-content-common": "^1.0.27",
    "wnpm-ci": "latest"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@wix/draft-js": "^0.10.105",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.19.1",
    "eslint-config-wix": "^1.1.18",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-lodash": "^2.7.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^21.2.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "prop-types": "^15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-test-renderer": "^15.6.1"
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  }
}
