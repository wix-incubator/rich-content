{
  "private": false,
  "name": "wix-rich-content-editor",
  "version": "1.0.0",
  "author": {
    "name": "Jonathan Bachman",
    "email": "jonathanb@wix.com"
  },
  "main": "dist/wix-rich-content-editor.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist/",
    "pretranspile": "npm run clean",
    "transpile": "babel -q src -d dist/src --copy-files",
    "prebuild": "npm run transpile",
    "build": "npm run build:santa && webpack --config webpack.prod.js ",
    "build:santa": "webpack --config webpack.santa.js",
    "build:analyze": "npm run build -- --progress --env.analyzeBundle",
    "build:dev": "webpack --config webpack.dev.js --colors --progress",
    "prewatch": "npm run clean",
    "watch": "npm run build:dev -- --watch",
    "watch:remote": "webpack --config webpack.remote.js --colors --progress --watch",
    "release": "wnpm-release --no-shrinkwrap",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "jest --verbose"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^draft-js$": "<rootDir>/node_modules/@wix/draft-js/dist/Draft.min.js",
      "^draft-js/.*$": "<rootDir>/node_modules/@wix/draft-js/dist/Draft.min.js",
      "^@wix/draft-js$": "<rootDir>/node_modules/@wix/draft-js/dist/Draft.min.js"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "classnames": "^2.2.5",
    "decorate-component-with-props": "^1.0.2",
    "draft-js-drag-n-drop-plugin": "^2.0.1",
    "draft-js-focus-plugin": "^2.0.1",
    "draft-js-linkify-plugin": "^2.0.1",
    "draft-js-plugins-editor": "^2.0.1",
    "draft-js-emoji-plugin": "^2.0.1",
    "i18next": "^10.5.0",
    "image-client-api": "1.51.0",
    "linkify-it": "^2.0.3",
    "lodash": "^4.17.4",
    "photography-client-lib": "^1.0.69",
    "react-i18next": "^7.5.0",
    "react-player": "^0.25.3",
    "react-sortable-hoc": "^0.6.8",
    "redraft": "^0.9.0",
    "wnpm-ci": "latest"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@wix/draft-js": "^0.10.13",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "eslint": "^4.15.0",
    "eslint-config-wix": "^1.1.16",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-lodash": "^2.5.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^21.2.1",
    "loader-utils": "^1.1.0",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "prop-types": "^15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-svg-loader": "^1.1.1",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-merge": "^4.1.1"
  },
  "eslintConfig": {
    "extends": "wix/react",
    "rules": {
      "no-console": "warning",
      "no-debugger": "warning"
    }
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  }
}
